                    font-size: 9pt;
                    vertical-align: top;
                    background-color: #FFFFFF
                }

                .texttablebold {
                    font-family: Arial, Helvetica;
                    font-size: 9pt;
                    color: #FFFFFF;
                    background-color: #000000
                }


                .ss12b {
                    font-family: Arial;
                    font-size: 12pt;
                    font-weight: bold;

                    color: #000000;
                    
                }
                .ss12c {
                    font-family: Arial;
                    font-size: 9pt;
                    background-color: #FFFFFF;
                    color: #000000;
                    
                }

                .columnas {
                    font-family: Arial;
                    background-color: #9C0434S;
                    color:#FFFFFF;
                    border: 0.5px solid gray;
            
                }

                

                .edoCuenta {
