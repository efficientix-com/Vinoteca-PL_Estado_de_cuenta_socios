                    background-color: #000000;
                }

                .blrb {
                    border-left: thin;
                    border-right: thin;
                    border-bottom: none;
                }

                .blrbt {
                    border-left: thin;
                    border-right: thin;
                    border-bottom: thin;
                    border-top: thin;
                }

                .brbt {
                    border-right: thin;
                    border-bottom: thin;
                    border-top: thin;
                }

                .brb {
                    border-right: thin;
                    border-bottom: none;
                }

                body,
                p,
                td,
                th {
                    font-family: Helvetica;
                    font-size: 11pt;
                }

                th {
                    font-weight: bold;
                    color: #ffffff;
                    background-color: #777777;
                }

                .headline {
                    font-family: Helvetica;
                    font-size: 17pt;
                    font-weight: bold;
                }

                .disclaimer {
